# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Translation Settings
DEFAULT_TARGET_LANGUAGE=ja

# Model Configuration (comma-separated, multiple models can be specified)
# Available models:
# - gemini-2.5-pro (2 RPM, 125,000 TPM, 50 RPD)
# - gemini-2.5-flash (10 RPM, 250,000 TPM, 250 RPD)
# - gemini-2.5-flash-preview (10 RPM, 250,000 TPM, 250 RPD)
# - gemini-2.5-flash-lite (15 RPM, 250,000 TPM, 1,000 RPD) Recommended
# - gemini-2.5-flash-lite-preview (15 RPM, 250,000 TPM, 1,000 RPD)
# - gemini-2.0-flash (15 RPM, 1,000,000 TPM, 200 RPD)
# - gemini-2.0-flash-lite (30 RPM, 1,000,000 TPM, 200 RPD)
GEMINI_MODELS=gemini-2.5-flash-lite,gemini-2.0-flash,gemini-2.0-flash-lite

# Model Strategy: "fallback" or "rotation"
# fallback: Automatically try the next model on error (recommended)
# rotation: Distribute requests evenly across models in rotation
MODEL_STRATEGY=fallback

# Database URL (for Prisma)
# Default: file:./prisma/data/translator.db
DATABASE_URL=file:./data/translator.db


